enum FeelingEnum {
    VERY_SAD
    SAD
    NEUTRAL
    HAPPY
    VERY_HAPPY
}
model DailyLog {
  id        String   @id @default(uuid())
  feeling   FeelingEnum @default(NEUTRAL)
  content   String? 
  createdAt DateTime @default(now())
  
  userId    String
  user      User     @relation(fields: [userId], references: [id])
}